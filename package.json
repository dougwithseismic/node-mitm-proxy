{
  "name": "@withseismic/mitm",
  "version": "2.1.0",
  "description": "Interactive MITM proxy with React Ink UI, rule engine, and first-run CA setup",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mitm": "./bin/mitm.js"
  },
  "files": [
    "bin",
    "dist",
    "rules",
    "types",
    ".claude-plugin",
    "skills",
    "agents"
  ],
  "keywords": [
    "mitm",
    "proxy",
    "http-proxy",
    "https-proxy",
    "man-in-the-middle",
    "intercept",
    "debug",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dougwithseismic/node-mitm-proxy"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build",

    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "commander": "^11.1.0",
    "hono": "^4.0.0",
    "ink": "^6.0.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "jiti": "^2.6.1",
    "minimatch": "^10.2.0",
    "node-forge": "^1.3.1",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/node-forge": "^1.3.14",
    "@types/react": "19.2.2",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
